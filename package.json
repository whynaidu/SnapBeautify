{
  "name": "snapbeautify-temp",
  "version": "0.1.3",
  "private": true,
  "main": "dist-electron/main.js",
  "author": "Vedant Naidu",
  "description": "Create stunning, professional screenshots in seconds.",
  "repository": "https://github.com/whynaidu/SnapBeautify.git",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "build:main": "tsc -p tsconfig.electron.json && cp electron/splash.html dist-electron/splash.html",
    "dev:electron": "concurrently -k \"cross-env BROWSER=none PORT=3002 npm run dev\" \"npm run build:main && wait-on http://localhost:3002 && cross-env ELECTRON_APP_URL=http://localhost:3002 electron . --no-sandbox\"",
    "build:electron": "npm run build:main && cross-env NEXT_PUBLIC_IS_ELECTRON=true npm run build && electron-builder",
    "dist": "npm run build:electron -- --linux --win",
    "dist:all": "npm run build:electron -- --linux --win --mac",
    "release": "npm run build:electron -- --win --publish always"
  },
  "build": {
    "appId": "com.snapbeautify.app",
    "productName": "SnapBeautify",
    "directories": {
      "output": "dist"
    },
    "files": [
      "dist-electron/**/*",
      "out/**/*",
      "package.json"
    ],
    "publish": {
      "provider": "github",
      "owner": "whynaidu",
      "repo": "SnapBeautify"
    },
    "linux": {
      "target": [
        "AppImage",
        "snap"
      ],
      "category": "Graphics",
      "icon": "build/icon.png"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "mac": {
      "target": "zip",
      "icon": "build/icon.png"
    }
  },
  "dependencies": {
    "electron-is-dev": "^3.0.1",
    "electron-updater": "^6.6.2"
  },
  "devDependencies": {
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "lucide-react": "^0.562.0",
    "next": "16.1.1",
    "next-themes": "^0.4.6",
    "png-to-ico": "^3.0.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-dropzone": "^14.3.8",
    "sharp": "^0.34.5",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "wait-on": "^9.0.3",
    "zustand": "^5.0.9"
  }
}
